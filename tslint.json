{
  "defaultSeverity": "error",
  "extends": [
    "tslint:latest",
    "tslint-react"
  ],
  "jsRules": {},
  "rules": {
    "quotemark": [
      true,
      "single",
      "jsx-double"
    ],
    "no-implicit-dependencies": [
      true,
      "dev"
    ],
    "no-submodule-imports": false,
    "semicolon": [
      true,
      "never"
    ],
    "object-literal-sort-keys": false,
    "max-line-length": [
      true,
      200
    ],
    "member-access": [
      false,
      "check-accessor"
    ]
  },
  "rulesDirectory": []
}