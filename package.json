{
  "name": "react-ts-boilerplate",
  "version": "1.0.0",
  "description": "React ❤ TS Boilerplate",
  "main": "index.js",
  "scripts": {
    "build": "npm run lint && rm -rf dist && webpack --config webpack.config.js --progress --colors --optimize-minimize",
    "dev": "NODE_ENV=development webpack-dev-server --mode=development --config ./webpack.config.js --content-base ./public",
    "lint": "tslint -c tslint.json -p tsconfig.json -t verbose 'src/**/*.{ts,tsx}'",
    "start": "npm run build && nodemon server.js",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "react",
    "typescript"
  ],
  "author": "kresnahendri",
  "license": "ISC",
  "repository": "github.com:kresnahendri/react-ts-boilerplate.git",
  "dependencies": {
    "axios": "^0.18.0",
    "bootstrap-4-grid": "^2.4.1",
    "express": "^4.16.4",
    "formik": "^1.5.1",
    "node-graceful": "^1.0.1",
    "nodemon": "^1.18.10",
    "normalize.css": "^8.0.1",
    "react": "^16.8.4",
    "react-dom": "^16.8.4",
    "react-redux": "^6.0.1",
    "react-router-dom": "^4.3.1",
    "redux": "^4.0.1",
    "redux-logger": "^3.0.6",
    "redux-observable": "^1.0.0",
    "rxjs": "^6.3.3",
    "styled-components": "^4.1.3",
    "typesafe-actions": "^3.2.1",
    "webpack": "^4.25.1",
    "yup": "^0.26.10"
  },
  "devDependencies": {
    "@types/axios": "^0.14.0",
    "@types/node": "^11.10.5",
    "@types/react": "^16.6.3",
    "@types/react-dom": "^16.0.9",
    "@types/react-redux": "^7.0.5",
    "@types/react-router-dom": "^4.3.1",
    "@types/redux-logger": "^3.0.7",
    "@types/styled-components": "^4.1.1",
    "@types/webpack-env": "^1.13.8",
    "@types/yup": "^0.26.11",
    "awesome-typescript-loader": "^5.2.1",
    "css-loader": "^2.1.0",
    "file-loader": "^3.0.1",
    "html-webpack-plugin": "^3.2.0",
    "prettier": "^1.15.3",
    "react-hot-loader": "^4.6.3",
    "source-map-loader": "^0.2.4",
    "style-loader": "^0.23.1",
    "ts-loader": "^5.3.0",
    "tslint": "^5.13.0",
    "tslint-react": "^3.6.0",
    "typescript": "^3.3.4000",
    "webpack-cli": "^3.1.2",
    "webpack-dev-middleware": "^3.4.0",
    "webpack-dev-server": ">=3.1.11",
    "webpack-hot-middleware": "^2.24.3"
  }
}